<!-- TEXT REVOLUTION -->
<Effect matchname="Text Revolution" name="$$$/AE/Preset/TextRevolution=Text Revolution">
    <Slider name="$$$/AE/Preset/TextTracking=Text Tracking" default="0.0" valid_min="0.0" valid_max="50000" slider_min="0" slider_max="100"/>
    <Group name="$$$/AE/Preset/RevolutionOptions=Revolution Options">
        <Checkbox name="$$$/AE/Preset/ReverseDirection=Reverse Direction" default="false" CANNOT_TIME_VARY="true"/>
        <Checkbox name="$$$/AE/Preset/HoldSpacePlacement=Hold Space Placement" default="false" CANNOT_TIME_VARY="true"/>
        <Slider name="$$$/AE/Preset/LoopRepeat=Loop Repeat" default="1" valid_min="0.0" valid_max="50000" slider_min="0" slider_max="100" precision="0" CANNOT_TIME_VARY="true"/>
        <Slider name="$$$/AE/Preset/PausedFrames=Paused Frames" default="0.0" valid_min="0.0" valid_max="50000" slider_min="0" slider_max="3600" precision="0" CANNOT_TIME_VARY="true"/>
    </Group>
</Effect>